<div id="application-events-index">
  <table class="table table-striped" id="url-response-times">
    <h2>Ordered Events</h2>
    <thead>
      <tr>
          <th data-field="event-name">Event Name</th>
          <th data-field="event-count">Event Count</th>
      </tr>
    </thead>
â€‹
    <tbody id="ordered-events">
      <% if event_names.empty? %>
        <p>No events have been defined</p>
      <% else %>
        <% event_names.each do |event, count| %>
        <tr>
          <td><a href="sources/<%= identifier %>/events/<%= event %>"><%= event %></a></td>
          <td><%= count %></td>
        </tr>
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
