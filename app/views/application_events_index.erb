<div id="application-events-index" class="row"><br><br>
  <div class="col s12 m12">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <div class="col s12 m12">
          <div class="card teal darken-1">
            <div class="card-content white-text">
              <table id="url-response-times">
                <h5>Ordered Events</h5>
                <thead>
                  <tr>
                    <th data-field="event-name">Event Name</th>
                    <th data-field="event-count">Event Count</th>
                  </tr>
                </thead>
                <tbody id="ordered-events">
                  <% if event_names.empty? %>
                    <p>No events have been defined</p>
                  <% else %>
                    <% event_names.each do |event, count| %>
                    <tr>
                      <td><a href="/sources/<%= identifier %>/events/<%= event %>"><%= event %></a></td>
                      <td><%= count %></td>
                    </tr>
                    <% end %>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
